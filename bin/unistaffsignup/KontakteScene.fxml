<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="640.0" prefWidth="826.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="unistaffsignup.StudentController">
   <children>
      <TabPane prefHeight="640.0" prefWidth="826.0">
        <tabs>
          <Tab text="Student">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <VBox alignment="CENTER" layoutX="1.0" layoutY="-4.0" prefHeight="618.0" prefWidth="232.0" spacing="30.0">
                           <children>
                              <TextField fx:id="idTextField" prefHeight="40.0" prefWidth="210.0" promptText="id">
                                 <font>
                                    <Font size="18.0" />
                                 </font>
                              </TextField>
                              <TextField fx:id="nameTextField" prefHeight="40.0" prefWidth="210.0" promptText="Name">
                                 <font>
                                    <Font size="18.0" />
                                 </font>
                              </TextField>
                              <TextField fx:id="adressTextField" prefHeight="40.0" prefWidth="210.0" promptText="Adress">
                                 <font>
                                    <Font size="18.0" />
                                 </font>
                              </TextField>
                              <TextField fx:id="phoneTextField" prefHeight="40.0" prefWidth="210.0" promptText="Telphone Number">
                                 <font>
                                    <Font size="18.0" />
                                 </font>
                              </TextField>
                              <TextField fx:id="departmentTextField" prefHeight="40.0" prefWidth="210.0" promptText="Department">
                                 <font>
                                    <Font size="18.0" />
                                 </font>
                              </TextField>
                              <Button fx:id="sendDataButton" mnemonicParsing="false" onAction="#studentButtonAction" prefHeight="26.0" prefWidth="118.0" text="Send Data" />
                           </children>
                        </VBox>
                        <TableView fx:id="studentTableView" layoutX="254.0" layoutY="-1.0" prefHeight="614.0" prefWidth="571.0">
                          <columns>
                            <TableColumn fx:id="idColumn" prefWidth="53.5999755859375" text="id" />
                              <TableColumn fx:id="nameColumn" prefWidth="130.4000244140625" text="Name" />
                              <TableColumn fx:id="adressColumn" prefWidth="126.39993286132812" text="Adress" />
                              <TableColumn fx:id="phoneColumn" prefWidth="119.199951171875" text="TelephoneNumber" />
                              <TableColumn fx:id="departmentColumn" prefWidth="144.0" text="Department" />
                          </columns>
                        </TableView>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Degree">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
