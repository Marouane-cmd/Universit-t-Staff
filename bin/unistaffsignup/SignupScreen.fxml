<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="459.0" prefWidth="645.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="unistaffsignup.SignUpController">
   <children>
      <VBox alignment="CENTER" layoutX="218.0" layoutY="-3.0" prefHeight="466.0" prefWidth="243.0" spacing="30.0">
         <children>
            <TextField fx:id="usernameTextField" prefHeight="55.0" prefWidth="214.0" promptText="user name">
               <font>
                  <Font size="25.0" />
               </font>
            </TextField>
            <PasswordField fx:id="passwordTextField" prefHeight="55.0" prefWidth="243.0" promptText="password">
               <font>
                  <Font size="25.0" />
               </font>
            </PasswordField>
            <TextField fx:id="departmentTextField" prefHeight="55.0" prefWidth="214.0" promptText="department">
               <font>
                  <Font size="25.0" />
               </font>
            </TextField>
            <Button fx:id="sendDataButton" mnemonicParsing="false" onAction="#sendDataAction" prefHeight="26.0" prefWidth="102.0" text="Send Data" />
            <Label text="enter department (cs , it or is ONLY please)" textFill="#e10909" />
         </children>
      </VBox>
      <Button fx:id="backButton" mnemonicParsing="false" onAction="#backButtonAction" prefHeight="26.0" prefWidth="92.0" text="Back" />
   </children>
</AnchorPane>
